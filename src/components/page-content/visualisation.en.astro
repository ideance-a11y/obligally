---
import WrapperDisclosure from '@/components/WrapperDisclosure.vue'
import { initI18n, getLangFromUrl } from '@/assets/i18n/i18n.ts'

const lang = getLangFromUrl(Astro.url)
const t = await initI18n(lang)
---

<img
  class="ice-infographic"
  alt={t('visualiser.image_alt')}
  src="/images/arbre.jpg"
/>
<WrapperDisclosure client:load>
  <h2 class="no-margin-bottom" slot="button">Description détaillée de l'infographie</h2>
  Description détaillée...
</WrapperDisclosure>
